{
  "version": 2,
  "name": "music-player-spa",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!music-player-SPA/).*)",
      "destination": "/music-player-SPA/index.html"
    }
  ]
}
